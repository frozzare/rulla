{
  "name": "rulla",
  "version": "1.0.0",
  "description": "Simple Rollup setup",
  "main": "index.js",
  "scripts": {
    "prebuild": "eslint lib test",
    "build": "rollup -c --environment BUILD:production",
    "watch": "rollup -c -w",
    "pretest": "rollup -c",
    "test": "mocha",
    "prepublish": "npm test"
  },
  "repository": "github.com/johnie/rulla",
  "keywords": [
    "es6",
    "modules",
    "rollup",
    "bundle",
    "browser"
  ],
  "author": "Johnie Hjelm <j@hjelm.im>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnie/rulla/issues"
  },
  "files": [
    "lib",
    "dist"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babelrc-rollup": "^3.0.0",
    "eslint": "^4.8.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "rollup": "^0.50.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-uglify": "^2.0.1"
  }
}
